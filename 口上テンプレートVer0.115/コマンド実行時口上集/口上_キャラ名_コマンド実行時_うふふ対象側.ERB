	;相手があなたの時
	IF TALENT:対象キャラ:恋慕
		SELECTCASE MAX(CUP:対象キャラ:快Ｃ, CUP:対象キャラ:快Ｂ)
			;各部位の累積が10000で絶頂、場合分けの数値は参考数値なのでイメージで変えてもらってOK
			CASE IS > 10000
				;即絶頂
				KSTR:(K++) = 
			CASE IS > 6000
				;かなり感じている
				KSTR:(K++) = 
			CASE IS > 3000
				;人並みに感じている
				KSTR:(K++) = 
			CASEELSE
				;まだ快楽に慣れていない
				KSTR:(K++) = 
		ENDSELECT
	ELSEIF TALENT:対象キャラ:セフレ
		KSTR:(K++) = 
	ELSE
		KSTR:(K++) = 
	ENDIF
ENDIF

