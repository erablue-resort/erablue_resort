			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フェラチオ3
			ENDSELECT
		ELSEIF MODE_確認_完全一致("フェラチオ系", 指定キャラ:1, MASTER) >= 0
			RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_フェラチオ
		ELSEIF コマンド履歴:0 == 208
			SELECTCASE RAND:5
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_まとめて全裸にする
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_まとめて全裸にする1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_まとめて全裸にする2
			CASE 3
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_まとめて全裸にする3
			CASE 4
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_まとめて全裸にする4
			;CASE 5
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_まとめて全裸にする5
			ENDSELECT

		ELSEIF (MODE_存在判定_完全一致("後背位", MASTER, 指定キャラ:0) || MODE_存在判定_完全一致("立ちバック", MASTER, 指定キャラ:0) || MODE_存在判定_完全一致("背面座位", MASTER, 指定キャラ:0)|| MODE_存在判定_完全一致("背面駅弁", MASTER, 指定キャラ:0)) && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:1, MASTER) 			
			SELECTCASE RAND:6
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_後背位
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_後背位1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_後背位2
			CASE 3
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_後背位3
			CASE 4
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_後背位4
			CASE 5
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_後背位5
        
			ENDSELECT
		ELSEIF (MODE_存在判定_完全一致("後背位", MASTER, 指定キャラ:1) || MODE_存在判定_完全一致("立ちバック", MASTER, 指定キャラ:1) || MODE_存在判定_完全一致("背面座位", MASTER, 指定キャラ:1)|| MODE_存在判定_完全一致("背面駅弁", MASTER, 指定キャラ:1)) && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:0, MASTER) 
			SELECTCASE RAND:4
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_後背位
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_後背位1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_後背位2
			CASE 3
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_後背位3
			ENDSELECT
		;後背位系以外のＶ挿入時
		ELSEIF MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:0, MASTER)  && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:1, MASTER) 
			SELECTCASE RAND:3
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_正常位
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_正常位1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_正常位2
			;CASE 3
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_正常位3
			;CASE 4
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_正常位4
			;CASE 5
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_正常位5
			ENDSELECT
		ELSEIF MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:1, MASTER)  && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:0, MASTER) 
			SELECTCASE RAND:6
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_正常位
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_正常位1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_正常位2
			CASE 3
