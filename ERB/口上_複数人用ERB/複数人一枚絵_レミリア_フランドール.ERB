;****************************************
;サイド領域の複数人一枚絵を登録するための関数
;****************************************
;・使い方
;「XXXX」を任意の文字列に書き換え、キャラの組み合わせと表示条件を入れる
;「XXXX」は既存の関数と被らないこと
;キャラの組み合わせは既存の関数と被らないこと、同じ組み合わせがないかどうか事前にチェックしてください
;処理的に、組み合わせのキャラが全員同じ場所にいないと条件は読まれません
;画像のサイズはおおよそ650*650程度が限度です
;****************************************

;--------------------------------------------------
;登録データ レミリア_フランドール
;登録データ
;--------------------------------------------------
@複数人一枚絵_登録データ_レミリア_フランドール(動作モード, 指定キャラ)
#DIMS 動作モード
#DIM REF 指定キャラ
#DIM ザーメンキス前フラグ

SELECTCASE 動作モード
	CASE "初期登録"
		;キャラの組み合わせを指定、5キャラまで指定可能
		指定キャラ:0 = キャラ検索("[永遠に紅い幼き月]レミリア")
		指定キャラ:1 = キャラ検索("[悪魔の妹]フランドール")
		; 指定キャラ:2 = キャラ検索("")
		; 指定キャラ:3 = キャラ検索("")
		; 指定キャラ:4 = キャラ検索("")
	CASE "画像指定"
		;表示条件を分岐しつつ画像名を指定する

		;以下はキャラ0とキャラ1が同時パイズリしている時の例
		IF MODE_確認_完全一致("パイズリ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("パイズリ系", 指定キャラ:1, PLAYER) >= 0
			;画像の位置を指定する
			;例えば指定キャラ:0のフォルダに画像が入れてあるなら、以下のように指定
			SELECTCASE RAND:7
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ1
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ2
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ
			CASE 3
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ3
			CASE 4
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ4
			CASE 5
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ5
			CASE 6
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ6
			;CASE 7
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ7
			;CASE 8
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ8
			;CASE 9
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ9
			;CASE 10
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルパイズリ10
			ENDSELECT
		;ダブルフェラ
		ELSEIF MODE_確認_完全一致("フェラチオ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("フェラチオ系", 指定キャラ:1, PLAYER) >= 0
			SELECTCASE RAND:5
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ1
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ2
			CASE 3
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ3
			CASE 4
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ4
			;CASE 5
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ5
			;CASE 6
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ6
			;CASE 7
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブルフェラチオ7
			ENDSELECT
		ELSEIF MODE_確認_完全一致("手コキ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("手コキ系", 指定キャラ:1, PLAYER) >= 0
			;画像の位置を指定する
			;指定キャラ:0のフォルダに指定
			SELECTCASE RAND:4
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ2
			CASE 3
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ3
			;CASE 4
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ4
			;CASE 5
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ5
			;CASE 6
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ6
