			ENDSELECT

		ELSEIF コマンド履歴:0== 6
			SELECTCASE RAND:4
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_同時に胸愛撫
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_同時に胸愛撫1
				CASE 2
