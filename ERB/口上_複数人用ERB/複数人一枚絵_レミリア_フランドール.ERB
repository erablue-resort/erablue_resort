			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ7
			;CASE 8
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ダブル手コキ8
			ENDSELECT
		;ELSEIF MODE_確認_完全一致("パイズリ系", 指定キャラ:0, MASTER) >= 0
		;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_パイズリ
		;ELSEIF MODE_確認_完全一致("パイズリ系", 指定キャラ:1, MASTER) >= 0
		;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_パイズリ
        ELSEIF MODE_確認_完全一致("手コキ系", 指定キャラ:0, MASTER) >= 0			
			SELECTCASE RAND:3
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_手コキ
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_手コキ1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_手コキ2
			;CASE 3
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_手コキ3
			;CASE 4
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_手コキ4
			ENDSELECT
		ELSEIF MODE_確認_完全一致("手コキ系", 指定キャラ:1, MASTER) >= 0
			SELECTCASE RAND:2
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_手コキ
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_手コキ1
			;CASE 2
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_手コキ2
			;CASE 3
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_手コキ3
			ENDSELECT
		ELSEIF MODE_確認_完全一致("フェラチオ系", 指定キャラ:0, MASTER) >= 0
			SELECTCASE RAND:2
			CASE 0
