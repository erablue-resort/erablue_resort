			CASE 5
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/フランドール_正常位5
			ENDSELECT
		ELSEIF コマンド履歴:0 == 20 && MODE_存在判定_順不同("キス系", 指定キャラ:0, 指定キャラ:1)
			IF SELECTCOM_NAME履歴:0 == "ザーメンキス"
				SELECTCASE RAND:5
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ザーメンシェア
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ザーメンシェア1
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/レミリア_フランドール/レミリア_フランドール_ザーメンシェア2
