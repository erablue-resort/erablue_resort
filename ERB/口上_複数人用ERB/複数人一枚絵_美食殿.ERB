			ENDSELECT
		ELSEIF MODE_確認_完全一致("手コキ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("手コキ系", 指定キャラ:1, PLAYER) >= 0
			;画像の位置を指定する
			;指定キャラ:0のフォルダに指定
			SELECTCASE RAND:8
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/ペコリーヌ_コッコロ/ペコリーヌ_コッコロ_ダブル手コキ1
			CASE 1
