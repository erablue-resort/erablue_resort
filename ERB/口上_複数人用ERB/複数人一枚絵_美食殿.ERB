;****************************************
;サイド領域の複数人一枚絵を登録するための関数
;****************************************
;・使い方
;「XXXX」を任意の文字列に書き換え、キャラの組み合わせと表示条件を入れる
;「XXXX」は既存の関数と被らないこと
;キャラの組み合わせは既存の関数と被らないこと、同じ組み合わせがないかどうか事前にチェックしてください
;処理的に、組み合わせのキャラが全員同じ場所にいないと条件は読まれません
;画像のサイズはおおよそ650*650程度が限度です
;****************************************

;--------------------------------------------------
;登録データキャル_コッコロ
;--------------------------------------------------
@複数人一枚絵_登録データ_キャル_コッコロ(動作モード, 指定キャラ)
#DIMS 動作モード
#DIM REF 指定キャラ
#DIM ザーメンキス前フラグ

SELECTCASE 動作モード
	CASE "初期登録"
		;キャラの組み合わせを指定、5キャラまで指定可能
		指定キャラ:0 = キャラ検索("[謎多き猫耳少女]キャル")
		指定キャラ:1 = キャラ検索("[小さなガイド役]コッコロ")
		; 指定キャラ:2 = キャラ検索("")
		; 指定キャラ:3 = キャラ検索("")
		; 指定キャラ:4 = キャラ検索("")
	CASE "画像指定"
		;表示条件を分岐しつつ画像名を指定する

		;以下はキャラ0とキャラ1が同時パイズリしている時の例
		IF MODE_確認_完全一致("パイズリ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("パイズリ系", 指定キャラ:1, PLAYER) >= 0
			;画像の位置を指定する
			;例えば指定キャラ:0のフォルダに画像が入れてあるなら、以下のように指定
			SELECTCASE RAND:3
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブルパイズリ1
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブルパイズリ2
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブルパイズリ
			ENDSELECT
		;ダブルフェラ
		ELSEIF MODE_確認_完全一致("フェラチオ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("フェラチオ系", 指定キャラ:1, PLAYER) >= 0
			SELECTCASE RAND:3
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブルフェラチオ1
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブルフェラチオ2
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブルフェラチオ
			ENDSELECT
		ELSEIF MODE_確認_完全一致("手コキ系", 指定キャラ:0, PLAYER) >= 0 && MODE_確認_完全一致("手コキ系", 指定キャラ:1, PLAYER) >= 0
			;画像の位置を指定する
			;指定キャラ:0のフォルダに指定
			SELECTCASE RAND:4
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブル手コキ
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブル手コキ1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブル手コキ2
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ダブル手コキ3
			ENDSELECT
		;ELSEIF MODE_確認_完全一致("フェラチオ系", 指定キャラ:0, MASTER) >= 0
		;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャルフェラ
		;ELSEIF MODE_確認_完全一致("フェラチオ系", 指定キャラ:1, MASTER) >= 0
		;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロフェラ
		ELSEIF コマンド履歴:0 == 208
			SELECTCASE RAND:3
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて全裸にする
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて全裸にする1
			CASE 2
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて全裸にする2

			ENDSELECT

		ELSEIF (MODE_存在判定_完全一致("後背位", MASTER, 指定キャラ:0) || MODE_存在判定_完全一致("立ちバック", MASTER, 指定キャラ:0) || MODE_存在判定_完全一致("背面座位", MASTER, 指定キャラ:0)|| MODE_存在判定_完全一致("背面駅弁", MASTER, 指定キャラ:0)) && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:1, MASTER) 			
			SELECTCASE RAND:1
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_後背位
			ENDSELECT
		ELSEIF (MODE_存在判定_完全一致("後背位", MASTER, 指定キャラ:1) || MODE_存在判定_完全一致("立ちバック", MASTER, 指定キャラ:1) || MODE_存在判定_完全一致("背面座位", MASTER, 指定キャラ:1)|| MODE_存在判定_完全一致("背面駅弁", MASTER, 指定キャラ:1)) && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:0, MASTER) 
			SELECTCASE RAND:2
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_後背位
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_後背位1
			ENDSELECT
		;後背位系以外のＶ挿入時
		;ELSEIF MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:0, MASTER)  && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:1, MASTER) 
			SELECTCASE RAND:1
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_正常位
			;CASE 1
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_正常位1
			;CASE 2
			;	RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_正常位2
			ENDSELECT
		ELSEIF MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:1, MASTER)  && !MODE_存在判定_順不同("Ｖ挿入系", 指定キャラ:0, MASTER) 
			SELECTCASE RAND:2
			CASE 0
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_正常位
			CASE 1
				RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_正常位1
			ENDSELECT
		ELSEIF コマンド履歴:0 == 20 && MODE_存在判定_順不同("キス系", 指定キャラ:0, 指定キャラ:1)
			IF SELECTCOM_NAME履歴:0 == "ザーメンキス"
				SELECTCASE RAND:5
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ザーメンシェア
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ザーメンシェア1
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ザーメンシェア2
				CASE 3
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ザーメンシェア3
				CASE 4
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_ザーメンシェア4
				ENDSELECT
				
			ELSE
			;漏れた場合
				SELECTCASE RAND:4
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_キス
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_キス1
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_キス2
				CASE 3
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_キス3
				ENDSELECT
			ENDIF

		;たぶんこれで「同時愛撫または同時胸愛撫の直後」になるはず
		;挿入モード系でないときを想定した画像なので後ろのほうの分岐に
		ELSEIF GETBIT(FLAG:モード管理, モードビット_同時) && (コマンド履歴:0 == 1 || コマンド履歴:0 == 6)
			SELECTCASE RAND:3
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_揉み比べ
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_揉み比べ1
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_揉み比べ2
			ENDSELECT

		ELSEIF コマンド履歴:0== 6
			SELECTCASE RAND:3
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_同時に胸愛撫
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_同時に胸愛撫1
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_同時に胸愛撫2
			ENDSELECT
		;ELSEIF コマンド履歴:0 == 7
		;	SELECTCASE RAND:2
		;		CASE 0
		;			RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_同時に乳首責め
		;		CASE 1
		;			RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_同時に乳首責め1
		;	ENDSELECT
		ELSEIF コマンド履歴:0== 11
			SELECTCASE RAND:5
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて抱きしめる
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて抱きしめる1
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて抱きしめる2
				CASE 3
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて抱きしめる3
				CASE 4
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_まとめて抱きしめる4
			ENDSELECT
		ELSEIF CFLAG:(指定キャラ:0):うふふ && CFLAG:(指定キャラ:1):うふふ
			IF TCVAR:(指定キャラ:0):膣内精液量 && TCVAR:(指定キャラ:1):膣内精液量
				SELECTCASE RAND:7
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服1
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服2
				CASE 2
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服3
				CASE 3
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服4
				CASE 4
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服5
				CASE 5
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服
				CASE 6
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ_征服6
				ENDSELECT
			ELSEIF TCVAR:(指定キャラ:0):膣内精液量
				SELECTCASE RAND:2
				CASE 0
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_征服1
				CASE 1
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_征服
				ENDSELECT
			ELSEIF TCVAR:(指定キャラ:1):膣内精液量
				SELECTCASE RAND:5
					CASE 0
						RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_征服
					CASE 1
						RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_征服1
					CASE 2
						RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_征服2
					CASE 3
						RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_征服3
					CASE 4
						RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/コッコロ_征服4
					
				ENDSELECT
			;ELSEIF  CSTR:(指定キャラ:0):着せ替え服 == "全裸"
			;	
			;	SELECTCASE RAND:3
			;	CASE 0
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_全裸にする
			;	CASE 1
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_全裸にする1
			;	CASE 2
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_全裸にする2
;
			;	ENDSELECT
			;;ELSE
			;	SELECTCASE RAND:4
			;	CASE 0
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ1
			;	CASE 1
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ2
			;	CASE 2
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ3
			;	CASE 3
			;		RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/キャル_コッコロ/キャル_コッコロ4
			;	ENDSELECT
			ENDIF
		ENDIF
ENDSELECT

;キャラたちが特定の場所にいる、場合の分岐は以下のようになる

