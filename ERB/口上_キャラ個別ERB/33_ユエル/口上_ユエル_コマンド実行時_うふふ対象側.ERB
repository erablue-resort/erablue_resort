@KOJO_COM_33_0(対象キャラ)
#DIM 対象キャラ
;取得快楽：Ｃ、Ｂ
;ランダム弱点コマンドの候補
CALL 口上変数初期化

IF PLAYER == MASTER
	;相手があなたの時
	IF TALENT:対象キャラ:恋慕
		SELECTCASE MAX(CUP:対象キャラ:快Ｃ, CUP:対象キャラ:快Ｂ)
			;各部位の累積が10000で絶頂、場合分けの数値は参考数値なのでイメージで変えてもらってOK
			CASE IS > 10000
				;即絶頂
				KSTR:(K++) = 「お゛っ_H_ ほお゛_H_ 壊れ_H_ いっぐ_H__H_ イクイクイクッ_H__H__H_ ぎゅっとして_H_ ウチのこと離さんといてぇ_H_」
			CASE IS > 6000
				;かなり感じている
				KSTR:(K++) = 「いひっ_H_ そこ_H_ もっと乱暴にしてっ_H__H_」
			CASE IS > 3000
				;人並みに感じている
				KSTR:(K++) = 「あっ_H_ んんっ_H_ くぅぅん_H__H_ もっと、もっとこすって_H_ 気持ちようして_H_」
			CASEELSE
				;まだ快楽に慣れていない
				KSTR:(K++) = 「もっと_H_ もっと強くして_H_ 乱暴に_H_ ウチのこと壊すくらい_H__H_」
		ENDSELECT
	ELSEIF TALENT:対象キャラ:セフレ
		SELECTCASE MAX(CUP:対象キャラ:快Ｃ, CUP:対象キャラ:快Ｂ)
			;各部位の累積が10000で絶頂、場合分けの数値は参考数値なのでイメージで変えてもらってOK
			CASE IS > 10000
				;即絶頂
				KSTR:(K++) = 「お゛っ_H_ ほお゛_H_ 壊れ_H_ いっぐ_H__H_ イクイクイクッ_H__H__H_ ぎゅっとして_H_ ウチのこと離さんといてぇ_H_」
			CASE IS > 6000
				;かなり感じている
				KSTR:(K++) = 「いひっ_H_ そこ_H_ もっと乱暴にしてっ_H__H_」
			CASE IS > 3000
				;人並みに感じている
				KSTR:(K++) = 「あっ_H_ んんっ_H_ くぅぅん_H__H_ もっと、もっとこすって_H_ 気持ちようして_H_」
			CASEELSE
				;まだ快楽に慣れていない
				KSTR:(K++) = 「もっと_H_ もっと強くして_H_ 乱暴に_H_ ウチのこと壊すくらい_H__H_」
		ENDSELECT
	ELSE
		SELECTCASE MAX(CUP:対象キャラ:快Ｃ, CUP:対象キャラ:快Ｂ)
			CASE IS > 10000
				KSTR:(K++) = 「イ゛くっ_H_ いくイくイクっ_H_ 激し_H_ お゛っ_H_ ほぉ_H_」
			CASE IS > 6000
				KSTR:(K++) = 「触って_H_ 乱暴に_H_ 勝手に気持ちようなっとるウチにお仕置きしてっ_H_」
			CASE IS > 3000
				KSTR:(K++) = 「あん_H_ んぅ_H_ ……声、出るの恥ずかしいんよ_H_」
			CASEELSE
				KSTR:(K++) = 「なんかわんちゃんみたいやなぁ_H_ あっ_H_ 冗談！ 冗談やから_H_」
		ENDSELECT
	ENDIF
ENDIF

