	SELECTCASE CUP:対象キャラ:快Ｓ
		;各部位の累積が10000で絶頂
		CASE IS > 10000
			;即絶頂
			KSTR:(K++) = 
		CASE IS > 6000
			;かなり感じている
			KSTR:(K++) = 
		CASE IS > 3000
			;人並みに感じている
