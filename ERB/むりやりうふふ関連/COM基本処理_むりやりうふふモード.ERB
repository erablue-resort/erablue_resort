@RAPE_COMTYPE(ARGS)
#FUNCTION
TSTR:複合コマンドタイプ受け渡し = 
IF STRFIND(ARGS, "&") >= 0
	TSTR:複合コマンドタイプ受け渡し = %ARGS%
	RETURNF 100
ELSEIF STRFIND(ARGS, "_") >= 0
	TSTR:複合コマンドタイプ受け渡し = %ARGS%
	RETURNF 101
ELSE
	RETURNF FINDELEMENT(コマンドタイプ_むりやりうふふモード,ARGS,,,1)
ENDIF

;こっちは数字から文字列で返す
@RAPE_COMTYPE_S(ARG)
#FUNCTIONS
SIF ARG >= 100
	RETURNF ""
RETURNF コマンドタイプ_むりやりうふふモード:ARG


