@USERCOM関数_804(動作モード)
#DIMS 動作モード

SELECTCASE 動作モード
	CASE "コマンド表示"
		PRINTC 能力表示(マスター)[804]
	CASE "コマンド条件"
		;コマンドの表示条件を処理する
		;表示したら1を返す
		RETURN 1
	CASE "コマンド動作"
		;コマンド選択時の動作を処理する
		CALL PRINT_STATE(MASTER)
ENDSELECT


@USERCOM関数_820(動作モード)
#DIMS 動作モード

SELECTCASE 動作モード
	CASE "コマンド表示"
		PRINTC 服を脱ぐ[820]
	CASE "コマンド条件"
		;コマンドの表示条件を処理する
		;表示するなら1を返す
		IF CFLAG:うふふ && TARGET > 0 && TALENT:PLAYER:脱衣不可 == 0
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	CASE "コマンド動作"
		;コマンド選択時の動作を処理する
		CALL PRINT_STATE(MASTER)
ENDSELECT





