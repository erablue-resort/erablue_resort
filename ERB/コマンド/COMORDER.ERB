	＋表示フラグ = 1
ENDIF
;同性愛（異性愛）
IF 同性愛性癖判定(TARGET,PLAYER)
	;性癖素質:同性愛
	IF 性癖素質:同性愛 > 0
		SIF ＋表示フラグ
			PRINT  + 
		実行値 +=性癖素質:同性愛 * 5
		IF TALENT:性愛傾向 == TALENT:性別
			PRINT 異性愛
		ELSE
			PRINT 同性愛
		ENDIF
		SELECTCASE 性癖素質:同性愛
			CASE 1
				PRINTFORM (興味)({性癖素質:同性愛 * 5})
			CASE 2
				PRINTFORM (愛好)({性癖素質:同性愛 * 5})
			CASE 3
				PRINTFORM (中毒)({性癖素質:同性愛 * 5})
		ENDSELECT
		＋表示フラグ = 1
	ENDIF
	;好奇心
	IF TALENT:性的興味 > 0
		SIF ＋表示フラグ
			PRINT  + 
		実行値 +=7
		PRINT 好奇心
		PRINT (7)
		＋表示フラグ = 1
	ENDIF
	;保守的
	IF TALENT:性的興味 < 0
		PRINT  - 
		実行値 -= 13
		PRINT 保守的
		PRINT (13)
		＋表示フラグ = 1
	ENDIF
ELSE
	;好奇心
	IF TALENT:性的興味 > 0
		SIF ＋表示フラグ
			PRINT  + 
		実行値 +=5
		PRINT 好奇心
		PRINT (5)
		＋表示フラグ = 1
	ENDIF
	;保守的
	IF TALENT:性的興味 < 0
		PRINT  - 
		実行値 -= 10
		PRINT 保守的
		PRINT (10)
		＋表示フラグ = 1
	ENDIF
ENDIF

