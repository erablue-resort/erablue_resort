@COM_ABLE372
;実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
SIF !TARGET
	RETURN 0
;個別のABLEを呼ぶ
TRYCCALLFORM COM_ABLE372_{NO:TARGET}
	SIF RESULT == 0
		RETURN 0
CATCH
	RETURN 0
ENDCATCH
RETURN 1

