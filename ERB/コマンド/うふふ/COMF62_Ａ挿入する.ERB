@CAN_Ａ体位(ARG)
#DIM 実行値
#DIM ＋表示フラグ

SIF CFLAG:TARGET:睡眠
	RETURN 1

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;縄で縛られている場合は無視（縄が無い時のみ実行判定を行う）
;IF TEQUIP:縄 == 0
	実行値 = 0
	＋表示フラグ = 0
	;すべての命令に共通の要素を考慮
	
	CALL COM_ORDER
	実行値 = RESULT
	＋表示フラグ = S

	;ABL:欲望
	IF ABL:欲望
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += ABL:欲望 * 3
		PRINT 欲望
		PRINTFORM LV{ABL:欲望}({ABL:欲望 * 3})
		＋表示フラグ = 1
	ENDIF
	IF RFLAG:指示モード対象あなたフラグ
		;ABL:Ｃ感覚
		IF ABL:Ｃ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ｃ感覚 * 3
			PRINT Ｃ感覚
			PRINTFORM LV{ABL:Ｃ感覚}({ABL:Ｃ感覚 * 3})
			＋表示フラグ = 1
		ENDIF
	ELSE
		;ABL:Ａ感覚
		IF ABL:Ａ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ａ感覚 * 2
			PRINT Ａ感覚
			PRINTFORM LV{ABL:Ａ感覚}({ABL:Ａ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
	ENDIF
	;快楽刻印
	IF MARK:快楽刻印
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += MARK:快楽刻印 * 3
		PRINT 快楽刻印
		PRINTFORM LV{MARK:快楽刻印}({MARK:快楽刻印 * 3})
		＋表示フラグ = 1
	ENDIF
	;PALAM:潤滑（の不足）
	IF RFLAG:指示モード対象あなたフラグ
		LOCAL = PALAM:MASTER:潤滑
	ELSE
		LOCAL = PALAM:潤滑
	ENDIF
	IF LOCAL < PALAMLV:3
		PRINT  - 
		実行値 -= 5
		PRINT 潤滑不足
		PRINT (5)
		＋表示フラグ = 1
	ENDIF
	;PALAM:欲情
	IF GETPALAMLV(PALAM:欲情,5)
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += GETPALAMLV(PALAM:欲情,5) * 3
		PRINT 欲情
		PRINTFORM LV{GETPALAMLV(PALAM:欲情,5)}({GETPALAMLV(PALAM:欲情,5) * 3})
		＋表示フラグ = 1
	ENDIF
	;恥じらい
	IF TALENT:羞恥心 > 0
		PRINT  - 
		実行値 -= 2
		PRINT 羞恥心
		PRINT (2)
		＋表示フラグ = 1
	ENDIF
	;快感の否定
	IF TALENT:快感応答 < 0 && !TALENT:恋慕
		PRINT  - 
		実行値 -= 5
		PRINT 快感の否定
		PRINT (5)
		＋表示フラグ = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += 15
		PRINT 恋慕
		PRINT (15)
		＋表示フラグ = 1
	ELSE
		;男嫌い（恋慕で無効）
		IF TALENT:性別嗜好 & 1 && TALENT:PLAYER:性別 == 2
			PRINT  - 
			実行値 -= 12
			PRINT 男嫌い
			PRINT (12)
			＋表示フラグ = 1
		ENDIF
		;女嫌い（恋慕で無効）
		IF TALENT:性別嗜好 & 2 && TALENT:PLAYER:性別 == 1
			PRINT  - 
			実行値 -= 12
			PRINT 女嫌い
			PRINT (12)
			＋表示フラグ = 1
		ENDIF
	ENDIF
	;Ａ経験
	IF RFLAG:指示モード対象あなたフラグ
		;C経験
		IF EXP:Ｃ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 5
			PRINT Ｃ経験
			PRINT 不足
			PRINT (5)
			＋表示フラグ = 1
		ENDIF
	ELSE
		IF EXP:Ａ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT Ａ経験
			PRINT 不足
			PRINT (15)
			＋表示フラグ = 1
		ENDIF
	ENDIF

	;調教者がふたなり
	IF (TALENT:PLAYER:性別 & 3) == 3
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += 4
		PRINT ふたなり
		PRINT (4)
		＋表示フラグ = 1
	ENDIF

	;媚薬
	IF TEQUIP:媚薬
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += 6
		PRINT 媚薬
		PRINT (6)
		＋表示フラグ = 1
	ENDIF

	;発情期
	IF CFLAG:発情期フラグ < 0
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += 4
		PRINT 発情中
		PRINT (4)
		＋表示フラグ = 1
	ENDIF

	;言いなり
	IF TCVAR:言いなり
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += 999
		PRINT あなたの言いなり
		PRINT (999)
		＋表示フラグ = 1
	ENDIF

	;合計を表示(ARG以上で実行)
	PRINT  = 
	PRINTV 実行値

	SIF 実行値 < ARG
		PRINT  < 
	SIF 実行値 == ARG
		PRINT  = 
	SIF 実行値 > ARG
		PRINT  > 
	PRINT 実行値
	PRINTV ARG

	WAIT

	;実行できない
	SIF 実行値 < ARG
		RETURN 0
;ENDIF
RETURN 1
