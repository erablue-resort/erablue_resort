;-------------------------------------------------
;セックス系コマンド
;-------------------------------------------------
@COMNAME62
#FUNCTIONS
TSTR:コマンド名受渡 = Ａ挿入する
SIF MODE_存在判定_順不同("Ａ挿入系", PLAYER, TARGET)
	TSTR:コマンド名受渡 = Ａ体位を変える

;弱点コマンド欄に専用の文字列を表示したい場合、これを設定すること
弱点コマンド欄表示名 = Ａ挿入する

SIF 派生名フラグ
	TSTR:コマンド名受渡 = Ａ挿入

;許可時の特別文章はここに記入
IF TFLAG:オートコマンドフラグ >= 4000
	;反転時、つまりあなたが入れる方
	許可取り文章 = %CALLNAME:TARGET%は%CALLNAME:PLAYER%に[Ａ挿入]して欲しいようだ…
ELSE
	許可取り文章 = %CALLNAME:PLAYER%は%CALLNAME:TARGET%に[Ａ挿入]したいようだ…
ENDIF

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_62
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("Ａ系")
SIF CFLAG:TARGET:睡眠
	TFLAG:コマンドタイプ受渡 = COMTYPE("セクハラ強")
睡眠中許可 = 1
;弱点コマンドカテゴリ = A
同時モード許可 = 3
同時モード拡張許可 = 1
オートコマンド許可 = 3
反転オート許可 = 3

@COM_TOOLTIP_62
LOCALS = <br>■Ａ挿入する<br>
LOCALS += "相手の尻穴に挿入し、アナルセックスを行うコマンド。<br>Ａ挿入系モード時、[Ａ体位を変える]に変化する。<br>Ａ挿入系モードとなり、モードを変更するか性行為を終了するまで持続的に快楽が発生する。"
LOCALS += "<br>COMタイプ：Ａ系<br>取得ソース：快Ａ・露出・接触・苦痛"
LOCALS += "<br>固有の取得経験：Ｃ性交経験_Ａ挿入(PLAYER)・Ａ性交経験(TARGET)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM62
#DIM 強さ初期値記録
#DIM 速さ初期値記録
#DIM ランダム候補, 20

IF TFLAG:オートコマンドフラグ
	;オートで実行する場合
	;ランダムで体位を決定して実行する
	VARSET ランダム候補, -1
	LOCAL:1 = 0
	FOR LOCAL, 0, 12
		TRYCALLFORM COM_ABLE62_{LOCAL}
		IF RESULT
			ランダム候補:(LOCAL:1) = LOCAL
			LOCAL:1 ++
		ENDIF
	NEXT
	LOCAL = ランダム候補:(RAND:(LOCAL:1))
	TRYCALLFORM COM62_{LOCAL}
	;メッセージ用に体位の種類を保存
	RFLAG:コマンド結果受渡し変数 = LOCAL
	RETURN 1
ELSEIF FLAG:モード管理 == 2
	IF RFLAG:コマンド結果受渡し変数 == 12
		TRYCALLFORM COM62_12(強さ初期値記録, 速さ初期値記録)
	ELSE
		強さ初期値記録 = 0
		速さ初期値記録 = 0
		$体位_LOOP2
		DRAWLINE
		PRINTL ■速さ・強さの初期値を選んでください
		PRINT 　　Ａ挿入強さ：
		PRINTBUTTON "[-]", 899
		FOR LOCAL, 0, 6
			IF 強さ初期値記録 >= LOCAL
				PRINTBUTTON "■", 900 + LOCAL
			ELSE
				PRINTBUTTON "□", 900 + LOCAL
			ENDIF
		NEXT
		PRINTBUTTON "[+]", "906"
		PRINT 　　
		PRINT Ａ挿入速さ：
		PRINTBUTTON "[-]", "909"
		FOR LOCAL, 0, 6
			IF 速さ初期値記録 >= LOCAL
				PRINTBUTTON "■", 910 + LOCAL
			ELSE
				PRINTBUTTON "□", 910 + LOCAL
			ENDIF
		NEXT
		PRINTBUTTON "[+]", 916
		PRINTL
		PRINTL
		PRINTBUTTONLC "[998]決定", 998
		PRINTBUTTONLC "[999]戻る", 999

		BINPUT

		LOCAL = RESULT
		SELECTCASE LOCAL
			CASE 900 TO 905
				強さ初期値記録 = LOCAL - 900
				GOTO 体位_LOOP2
			CASE 910 TO 915
				速さ初期値記録 = LOCAL - 910
				GOTO 体位_LOOP2
			CASE 899
				強さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
				GOTO 体位_LOOP2
			CASE 906
				強さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
				GOTO 体位_LOOP2
			CASE 909
				速さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
				GOTO 体位_LOOP2
			CASE 916
				速さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
				GOTO 体位_LOOP2
			CASE 999
				RETURN -1
		ENDSELECT
		LOCAL:10 = TARGET
		FOR LOCAL, 0, 同時モード_選択数
			SIF 同時モード_選択キャラ:LOCAL < 0
				BREAK
			RESULT = 0
			TARGET = 同時モード_選択キャラ:LOCAL
			TRYCALLFORM COM_ABLE62_12
			IF RESULT == 0
				TARGET = LOCAL:10
				RETURN -1
			ENDIF
			RESULT = 1
			TRYCALLFORM CAN_Ａ体位12(1)
			IF !RESULT
				TRYCALLFORM CAN_Ａ体位12(0)
				RETURN -1
			ENDIF
		NEXT
		TARGET = LOCAL:10

		TRYCALLFORM COM62_12(強さ初期値記録, 速さ初期値記録)
		;メッセージ用に体位の種類を保存
		RFLAG:コマンド結果受渡し変数 = 12
	ENDIF
	RETURN 1
ELSE
	IF MODE_存在判定_完全一致("Ａ挿入系", PLAYER, TARGET)
		強さ初期値記録 = A挿入強さ
		速さ初期値記録 = A挿入速さ
	ELSE
		強さ初期値記録 = 0
		速さ初期値記録 = 0
	ENDIF
	$体位_LOOP
	DRAWLINE
	PRINTL ■体位を選んでください
	PRINTL
	FOR LOCAL, 0, 20
		TRYCCALLFORM COM_ABLE62_{LOCAL}
			IF RESULT
				SELECTCASE LOCAL
					CASE 0
						PRINTBUTTONLC "[0]Ａ正常位", 0
					CASE 1
						PRINTBUTTONLC "[1]Ａ後背位", 1
					CASE 2
						PRINTBUTTONLC "[2]Ａ寝バック", 2
					CASE 3
						PRINTBUTTONLC "[3]Ａ対面座位", 3
					CASE 4
						PRINTBUTTONLC "[4]Ａ背面座位", 4
					CASE 5
						PRINTBUTTONLC "[5]Ａ騎乗位", 5
					CASE 6
						PRINTBUTTONLC "[6]Ａ側位", 6
					CASE 7
						PRINTBUTTONLC "[7]Ａ屈曲位", 7
					CASE 8
						PRINTBUTTONLC "[8]Ａ駅弁", 8
					CASE 9
						PRINTBUTTONLC "[9]Ａ背面駅弁", 9
					CASE 10
						PRINTBUTTONLC "[10]Ａ対面立位", 10
					CASE 11
						PRINTBUTTONLC "[11]Ａ立ちバック", 11
				ENDSELECT
			ENDIF
		CATCH
			CONTINUE
		ENDCATCH
	NEXT

	PRINTL
	PRINTL
	PRINTL ■速さ・強さの初期値を選んでください
	PRINT 　　Ａ挿入強さ：
	PRINTBUTTON "[-]", 899
	FOR LOCAL, 0, 6
		IF 強さ初期値記録 >= LOCAL
			PRINTBUTTON "■", 900 + LOCAL
		ELSE
			PRINTBUTTON "□", 900 + LOCAL
		ENDIF
	NEXT
	PRINTBUTTON "[+]", "906"
	PRINT 　　
	PRINT Ａ挿入速さ：
	PRINTBUTTON "[-]", "909"
	FOR LOCAL, 0, 6
		IF 速さ初期値記録 >= LOCAL
			PRINTBUTTON "■", 910 + LOCAL
		ELSE
			PRINTBUTTON "□", 910 + LOCAL
		ENDIF
	NEXT
	PRINTBUTTON "[+]", 916
	PRINTL
	PRINTL
	PRINTBUTTONLC "[999]戻る", 999

	$INPUT_LOOP
	INPUT

	LOCAL = RESULT
	SELECTCASE LOCAL
		CASE 0 TO 11
			TRYCALLFORM COM_ABLE62_{LOCAL}
			IF RESULT
				IF TARGET == MASTER
					TARGET = PLAYER
					PLAYER = MASTER
					RFLAG:指示モード対象あなたフラグ = 1
					TRYCALLFORM CAN_Ａ体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_Ａ体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
					RFLAG:指示モード対象あなたフラグ = 0
					PLAYER = TARGET
					TARGET = MASTER
				ELSE
					TRYCALLFORM CAN_Ａ体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_Ａ体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
				ENDIF
				TRYCALLFORM COM62_{LOCAL}(強さ初期値記録, 速さ初期値記録)
				;メッセージ用に体位の種類を保存
				RFLAG:コマンド結果受渡し変数 = LOCAL
				RETURN 1
			ENDIF
			GOTO INPUT_LOOP
		CASE 900 TO 905
			強さ初期値記録 = LOCAL - 900
			GOTO 体位_LOOP
		CASE 910 TO 915
			速さ初期値記録 = LOCAL - 910
			GOTO 体位_LOOP
		CASE 899
			強さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
			GOTO 体位_LOOP
		CASE 906
			強さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
			GOTO 体位_LOOP
		CASE 909
			速さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
			GOTO 体位_LOOP
		CASE 916
			速さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
			GOTO 体位_LOOP
		CASE 999
			A挿入強さ = 強さ初期値記録
			A挿入速さ = 速さ初期値記録
			RETURN -1
		CASEELSE
			GOTO INPUT_LOOP
	ENDSELECT
ENDIF

