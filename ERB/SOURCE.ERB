			PRINTFORMW %CALLNAME:対象キャラ%の干渉ランクが上昇した！
		ENDIF
	ENDIF
	SKIPDISP 0
	LOCALS '= CHECK_体位(対象キャラ)
	SIF LOCALS != "" && GROUPMATCH(TCVAR:対象キャラ:被射精位置, 1, 2)
		CALL アニメ表示_射精(LOCALS, PLAYER)

	;警戒度表示
	IF BASE:対象キャラ:警戒 != TCVAR:対象キャラ:前ターン警戒度
		IF BASE:対象キャラ:警戒 > TCVAR:対象キャラ:前ターン警戒度
